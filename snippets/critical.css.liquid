 * -----------------------------------------------------------------------------
 * Primary grid system based on materialize.css.
 * - https://sass-mq.github.io/sass-mq
 *
 * Why we use use em values for media queries.
 * https://zellwk.com/blog/media-query-units/
 */
body {
  overflow-y: scroll; }

.page-width {
  max-width: 73rem;
  margin-left: auto;
  margin-right: auto; }
  @media (max-width: 46.24em) {
    .page-width {
      overflow-x: hidden; } }
  .page-width--small {
    max-width: 60rem; }
  .page-width--tablet {
    max-width: 46.25rem; }
  .page-width--mobile {
    max-width: 20rem; }
  @media (max-width: 46.24em) {
    .page-width--overflow-x-hide {
      overflow-x: hidden; } }

.page-padding {
  padding-left: 1rem;
  padding-right: 1rem; }
  @media (min-width: 46.25em) {
    .page-padding {
      padding-left: 1.5rem;
      padding-right: 1.5rem; } }
  @media (min-width: 61.25em) {
    .page-padding {
      padding-left: 2rem;
      padding-right: 2rem; } }

.template-index .main-page-wrap {
  padding: 0;
  max-width: none; }

.vertical-padding {
  padding-top: 0.75rem;
  padding-bottom: 0.75rem; }
  .vertical-padding--section {
    padding-top: calc(2.5rem * 0.75);
    padding-bottom: calc(2.5rem * 0.75); }
  @media (min-width: 46.25em) {
    .vertical-padding {
      padding-top: 1.5rem;
      padding-bottom: 1.5rem; }
      .vertical-padding--section {
        padding-top: calc(3rem * 0.75);
        padding-bottom: calc(3rem * 0.75); } }
  @media (min-width: 61.25em) {
    .vertical-padding {
      padding-top: 2rem;
      padding-bottom: 2rem; }
      .vertical-padding--small {
        padding-top: calc(2rem / 2);
        padding-bottom: calc(2rem / 2); }
      .vertical-padding--section {
        padding-top: cal(3.5rem * 0.75);
        padding-bottom: cal(3.5rem * 0.75); } }

